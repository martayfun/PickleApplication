<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title</title>
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <link rel="stylesheet" href="~/Areas/Admin/Content/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Content/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Content/Ionicons/css/ionicons.min.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Content/adminlte/AdminLTE.min.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Content/iCheck/square/blue.css" />
    @RenderSection("styles", required: false)
</head>
<body class="hold-transition login-page">
    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Areas/Admin/Content/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Areas/Admin/Content/iCheck/icheck.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>